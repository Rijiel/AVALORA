@{
	if (TempData[SD.TEMPDATA_SUCCESS] != null)
	{
		<script src="~/lib/jquery/dist/jquery.min.js"></script>
		<script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>

		<script>
			toastr.success('@TempData[SD.TEMPDATA_SUCCESS]');
		</script>
	}

	if (TempData[SD.TEMPDATA_ERROR] != null)
	{
		<script src="~/lib/jquery/dist/jquery.min.js"></script>
		<script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>

		<script>
			toastr.error('@TempData[SD.TEMPDATA_ERROR]');
		</script>
	}
}
