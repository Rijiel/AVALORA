@{
	if (TempData[SD.TEMPDATA_SUCCESS] != null)
	{
		<script src="~/lib/jquery/dist/jquery.min.js"></script>
		<script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>

		<script>
			toastr.options = {
				"showDuration": "300",
				"hideDuration": "1000",
				"timeOut": "1500",
				"extendedTimeOut": "1000"
			}

			toastr.success('@TempData[SD.TEMPDATA_SUCCESS]');
		</script>
	}

	if (TempData[SD.TEMPDATA_ERROR] != null)
	{
		<script src="~/lib/jquery/dist/jquery.min.js"></script>
		<script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>

		<script>
			toastr.options = {
				"showDuration": "300",
				"hideDuration": "1000",
				"timeOut": "1500",
				"extendedTimeOut": "1000"
			}

			toastr.error('@TempData[SD.TEMPDATA_ERROR]');
		</script>
	}
}
